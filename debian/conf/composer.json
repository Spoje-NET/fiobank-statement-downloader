  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "info@vitexsoftware.cz"
    }
  ],
  "require": {
    "deb/ease-core": "*",
    "mhujer/fio-api-php": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "SpojeNet\\FioApi\\": "/usr/share/fiobank-statement-downloader/SpojeNet/FioApi"
